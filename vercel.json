{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://sm.cms.jiminy.co.nz https://*.jiminy.co.nz"
        }
      ]
    }
  ]
}
